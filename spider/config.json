{
    "url": "http://www.xigutang.com/zhuanti/libai.html",
    "selector": [{
        "root": ".speciallist .tbox",
        "element": "a",
        "prop": {
            "target": "_blank"
        },
        "output": {
            "attr": "href",
            "text": "text"
        },
        "action": {
            "recursive": true,
            "selector": [
                {
                    "root": ".title",
                    "element": "h1",
                    "output": {
                        "text": "text",
                        "type": "h1"
                    }
                },
                {
                    "root": ".content",
                    "element": "td p",
                    "nth": 0,
                    "output": {
                        "text": "text",
                        "type": "text"
                    }
                }
            ]
        }
    }]
}